<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="f0"
            type="String" />
        <variable
            name="f1"
            type="String" />
        <variable
            name="f2"
            type="String" />
        <variable
            name="f3"
            type="String" />
        <variable
            name="m0"
            type="String" />
        <variable
            name="m1"
            type="String" />
        <variable
            name="m2"
            type="String" />
        <variable
            name="m3"
            type="String" />

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <EditText
            android:id="@+id/editTextf3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="number"
            android:text="@{f3}"
            app:layout_constraintEnd_toStartOf="@+id/editTextm0"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/guideline2"
            app:layout_constraintTop_toBottomOf="@+id/editTextf2" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Mayores de 50 Años"
            app:layout_constraintBottom_toBottomOf="@+id/editTextf3"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/editTextf3" />

        <EditText
            android:id="@+id/editTextm3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="number"
            android:text="@{m3}"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/editTextf0"
            app:layout_constraintTop_toBottomOf="@+id/editTextm2" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Mayores de 15 a 49 Años"
            app:layout_constraintBottom_toBottomOf="@+id/editTextf2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/editTextf2" />

        <EditText
            android:id="@+id/editTextf2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="number"
            android:text="@{f2}"
            app:layout_constraintEnd_toStartOf="@+id/editTextm0"
            app:layout_constraintStart_toStartOf="@+id/guideline2"
            app:layout_constraintTop_toBottomOf="@+id/editTextf1" />

        <EditText
            android:id="@+id/editTextm2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="number"
            android:text="@{m2}"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/editTextf0"
            app:layout_constraintTop_toBottomOf="@+id/editTextm1" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Niños de 6 a 14 Años"
            app:layout_constraintBottom_toBottomOf="@+id/editTextf1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/editTextf1" />

        <EditText
            android:id="@+id/editTextf1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="number"
            android:text="@{f1}"
            app:layout_constraintEnd_toStartOf="@+id/editTextm0"
            app:layout_constraintStart_toStartOf="@+id/guideline2"
            app:layout_constraintTop_toBottomOf="@+id/editTextf0" />

        <EditText
            android:id="@+id/editTextm1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="number"
            android:text="@{m1}"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/editTextf0"
            app:layout_constraintTop_toBottomOf="@+id/editTextm0" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="19dp"
            android:text="Mujeres"
            app:layout_constraintEnd_toEndOf="@+id/editTextf0"
            app:layout_constraintStart_toStartOf="@+id/editTextf0"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="19dp"
            android:text="Hombres"
            app:layout_constraintEnd_toEndOf="@+id/editTextm0"
            app:layout_constraintStart_toStartOf="@+id/editTextm0"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Menores de 0 a 5 Años"
            app:layout_constraintBottom_toBottomOf="@+id/editTextf0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/editTextf0" />

        <EditText
            android:id="@+id/editTextm0"
            android:layout_width="0dp"
            android:layout_height="45dp"
            android:ems="10"
            android:inputType="number"
            android:text="@{m0}"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/editTextf0"
            app:layout_constraintTop_toBottomOf="@+id/textView2" />

        <EditText
            android:id="@+id/editTextf0"
            android:layout_width="0dp"
            android:layout_height="45dp"
            android:ems="10"
            android:inputType="number"
            android:text="@{f0}"
            app:layout_constraintEnd_toStartOf="@+id/editTextm0"
            app:layout_constraintStart_toStartOf="@+id/guideline2"
            app:layout_constraintTop_toBottomOf="@+id/textView" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_begin="160dp"
            app:layout_constraintStart_toStartOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>