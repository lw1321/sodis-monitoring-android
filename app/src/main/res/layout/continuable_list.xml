<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools">

    <include layout="@layout/list"/>


    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/navigation_forward_button_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:src="@drawable/ic_arrow_forward_white_24dp"
        app:backgroundTint="@color/colorPrimary"
        app:fabSize="auto"
        android:layout_marginEnd="@dimen/padding_edge"
        android:layout_marginRight="@dimen/padding_edge"
        android:layout_marginStart="@dimen/padding_edge"
        android:layout_marginLeft="@dimen/padding_edge"
        android:layout_marginBottom="@dimen/padding_edge"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>